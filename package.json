  "keywords": [
    "bittorrent",
    "p2p",
    "peer",
    "peer-to-peer",
    "stream",
    "torrent",
    "tracker",
    "wire"
  ],
  "license": "MIT",
  "scripts": {
    "build": "babel src -d lib",
    "release": "rimraf ./lib && mkdir ./lib && rollup-worker -c .config/fssrc.js",
    "update-authors": "./tools/update-authors.sh",
    "test": "npm run -s build && standard && tape test/*.js",
    "prepare": "npm run -s build"
  },
  "files": [
    "lib/",
    "index.d.ts"
  ]
